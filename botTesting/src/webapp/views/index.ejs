<html>

<head>
  <title>
    Generate TestData and Test Your chatBots..
  </title>
  <% include partials/header %>
</head>

<body>
<% include partials/nav %>

  <div class="container-fluid">
    <!-- <div class="jumbotron" style="    padding-bottom: 5px;">
      <h1>Generate TestData</h1>
      <p class="lead">Generate TestData and Test Your chatBots..</p>
      
    </div> -->
      <div class="col-md-12 alertInfo" style="z-index: 100;width:100%">
          <!-- SHOW ALERTS MESSAGES-->
      </div>

    <div class="row">
      <div class="col-md-4 testflow-container">
        <div class="row testflow-toolbar-row">
          <div class="col-sm-12">
            <div class="btn-group chat-toolbar" role="group" aria-label="...">
              <button id="chat-clear" class="btn btn-info disabled" type="button" onclick="reset()">
                  Reset/Refresh
                </button> &nbsp;&nbsp;&nbsp;&nbsp;
              <button id="chat-clear" class="btn btn-info disabled" type="button" onclick="deleteActiveFlow()">
                  Delete
                </button>
            </div>
          </div>
        </div>
          <div class="row testflow-toolbar-row">
              <div class="col-sm-12">
                  <div class="btn-group chat-toolbar" role="group" aria-label="...">
                      <button class="btn btn-info test_flow" type="button" onclick="testFlow()" title="testFlow">
                          Test Flow &raquo;
                      </button>
                  </div>
              </div>
          </div>
        <div class="row testflow-toolbar-row">
          <div class="col-sm-12">
            <div class="btn-group chat-toolbar" role="group" aria-label="...">
              <button class="btn btn-info create_save_flow" type="button" onclick="create_save_flow()" title="createNewFlow">
                  Create New Flow &raquo;
                </button>
            </div>
          </div>
        </div>
        <div class="row chat-window-row">

          <div class="col-md-12">
            <!-- <a class="btn create_save_flow" href="#" onclick="create_save_flow()" title="createNewFlow">Create New Flow &raquo;</a> -->
            <!--loop -->
            <% for( var i=0;i < locals.flowIds.length;i++) {%>
              <a class="list-group-item" onclick="viewFlowById('<%= locals.flowIds[i].key %>')" href="#" title="View <%= locals.flowIds[i].key %>">
                <%= locals.flowIds[i].key %> 
                <% if(locals.flowIds[i].status == 'saved') { %>
                  <span>üìÅ  </span>
                <% } else{ %>
                  <span>‚úçÔ∏è </span>
                <% } %>
              </a>
              <% } %>
          </div>
        </div>
      </div>

      <!-- chatWindow: -->
      <div class="col-md-8 chat-window-container" style="display: none">
        <div class="row chat-window-toolbar-row">
          <div class="col-md-6 col-6 ">
            <div class="input-group">
              <input type="text" class="form-control inputSenderId" title="senderId" placeholder="senderId" aria-label="senderId">
              <span class="input-group-btn">
                <button class="btn validSenderId" title="required" type="button">‚úÖ</button>
              </span>
            </div>
          </div>
          <div class="col-md-6 col-6 ">
            <div class="input-group">
              <input type="text" class="form-control inputPageId" title="pageId" placeholder="pageId" aria-label="pageId">
              <span class="input-group-btn">
                <button class="btn validPageId" title="required" type="button">‚úÖ</button>
              </span>
            </div>
          </div>
        </div>
        <div class="row chat-window-toolbar-row">
          <div class="col-md-6 col-6 ">
            <div class="input-group">
              <input type="text" class="form-control inputAppId" title="appId" placeholder="appId" aria-label="appId">
              <span class="input-group-btn">
                <button class="btn validAppId" title="required" type="button">‚úÖ</button>
              </span>
            </div>
          </div>
          <div class="col-md-6 col-6 ">
            <div class="input-group">
              <input type="text" class="form-control inputFlowId" title="flowId" placeholder="flowId" aria-label="flowId">
              <span class="input-group-btn">
                <button class="btn validFlowId" title="optional" type="button">‚úÖ</button>
              </span>
            </div>
          </div>
        </div>
        <div class="row chat-window-toolbar-row">
          <div class="col-md-12">
            <div class="input-group">
              <textarea type="text" class="form-control inputFlowDescription" title="Description" placeholder="Description..." aria-label="flowDescription"></textarea>
              <span class="input-group-btn">
                <button class="btn validFlowDescription" title="optional" type="button">‚úç</button>
              </span>
            </div>
            <div style="text-align: center;">
              <button class="btn btn-secondary edit_update_flowInfo" href="#" title="create" onclick="edit_update_flowInfo()" role="save">Update details &raquo;</button>
            </div>
          </div>
        </div>
        <div class="container">
          <div class="col-md-12">
            <br/>
            <% include partials/chat %>
          </div>
        </div>
      </div>
      <div class="col-md-8 outer-hint">
        <div class="jumbotron">
          <div class="container">
            <h1 class="display-3">Hello, Tester!</h1>
            <p class="outer-hint-message">You can generate TestData using this</p>
          </div>
        </div>
      </div>
    </div>


  </div>
  <footer class="footer">
    <div class="container">
      <span class="text-muted">footer.</span>
    </div>
  </footer>
</body>

</html>